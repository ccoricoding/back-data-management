{
    "framework": "vite",
    "buildCommand": "vite build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/db-usage",
            "destination": "/api/db-usage.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}